{
    "model": "gpt-4o",
    "show_line_numbers": false,
    // https://textual.textualize.io/guide/input/#bindings or `$ textual keys`
    // keybindings are bound to e.g. `action_copy_text(self)`
    "keybindings": [
        [
            "ctrl+p",
            "submit()",
            "submit"
        ],
        [
            "ctrl+o",
            "toggle_input",
            "expand text area"
        ],
        [
            "ctrl+r",
            "reset_chat_session()",
            "reset session"
        ],
        [
            "ctrl+l",
            "focus_input()",
            "focus input"
        ],
        [
            // "quit" binding should match tmux.conf
            "ctrl+x",
            "quit",
            "detach"
        ]
    ]
}